/*------------------------------------------------------------------------------
 *CSS VIEW ALL FABRIC
 *------------------------------------------------------------------------------*/

.view-all-fabric{width: 100%;height: 100%;background: rgba(0,0,0,.5)}
.view-all-fabric .menuL{height: 200px;margin: auto auto 10%;overflow: hidden;position: relative;width: 200px;}
