#menu #subMenu #subMenu-contrast li:hover{width: 87%}

.txtLinkCheckout{background: url("../img/bg/bg-checkout.png") no-repeat scroll center top;margin: 10px auto;font-size: 111%;letter-spacing: 0.25px;font-weight: 500}
#menuMain-optionCart{padding: 5%;color:skyblue;position: relative;font-size: 78%;height: 100px}
